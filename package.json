{
  "name": "sapience-dual-agent",
  "version": "1.0.0",
  "description": "Sapience Dual Agent - TypeScript project for AI agent systems",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "cli": "ts-node src/index.ts",
    "watch": "tsc --watch",
    "lint": "next lint",
    "format": "prettier --write src/**/*.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:dome": "ts-node src/test-dome-groq.ts",
    "test:direct": "ts-node src/test-direct-groq.ts",
    "test:dome-debug": "ts-node src/test-dome-debug.ts",
    "test:dry-run": "tsx src/test-dry-run.ts",
    "dry-run": "tsx src/trading-dry-run.ts",
    "dry-run:quick": "cross-env MAX_TRADES=3 tsx src/trading-dry-run.ts",
    "ethereal:dry-run": "tsx src/ethereal-dry-run.ts",
    "perps:dry-run": "tsx src/ethereal-dry-run.ts",
    "clean": "rimraf dist .next",
    "prebuild": "npm run clean",
    "test:trading-agent": "tsx scripts/test-trading-agent.ts",
    "trading-agent:dry-run": "DRY_RUN=true tsx src/agents/trading-agent.ts",
    "trading-agent:live": "DRY_RUN=false tsx src/agents/trading-agent.ts",
    "test:ethereal": "tsx scripts/test-ethereal-trading.ts",
    "test:ethereal:dry-run": "tsx scripts/test-ethereal-dry-run.ts",
    "optimize:agent": "tsx scripts/optimize-agent.ts",
    "trade:live": "tsx scripts/trade-live-ethereal.ts",
    "generate:dashboard": "tsx scripts/generate-dashboard.ts",
    "verify-trades": "tsx scripts/verify-trades.ts"
  },
  "keywords": [
    "ai",
    "agent",
    "typescript",
    "dual-agent",
    "sapience"
  ],
  "author": "21givenchy",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.32.1",
    "@dome-api/sdk": "0.18.0",
    "@privy-io/server-auth": "^1.14.2",
    "@sapience/sdk": "^0.0.1",
    "@tanstack/react-query": "^5.59.0",
    "@wagmi/core": "^2.15.3",
    "axios": "^1.7.9",
    "dotenv": "^16.4.7",
    "ethers": "^6.13.4",
    "express": "^4.21.0",
    "graphql-request": "^7.1.2",
    "groq-sdk": "^0.8.0",
    "next": "16.1.1",
    "node-fetch": "^2.7.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "viem": "^2.43.3",
    "wagmi": "^2.13.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.23",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.2",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@typescript-eslint/eslint-plugin": "^8.18.0",
    "@typescript-eslint/parser": "^8.18.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.18.0",
    "eslint-config-next": "16.1.1",
    "jest": "^29.7.0",
    "postcss": "^8.4.47",
    "prettier": "^3.4.2",
    "rimraf": "^6.0.1",
    "tailwindcss": "^3.4.15",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
