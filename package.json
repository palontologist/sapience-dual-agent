{
  "name": "sapience-dual-agent",
  "version": "1.0.0",
  "description": "Sapience Dual Agent - TypeScript project for AI agent systems",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "watch": "tsc --watch",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:dome": "ts-node src/test-dome-groq.ts",
    "test:direct": "ts-node src/test-direct-groq.ts",
    "test:dome-debug": "ts-node src/test-dome-debug.ts",
    "clean": "rimraf dist",
    "prebuild": "npm run clean"
  },
  "keywords": [
    "ai",
    "agent",
    "typescript",
    "dual-agent",
    "sapience"
  ],
  "author": "21givenchy",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@dome-api/sdk": "^0.17.0",
    "@privy-io/server-auth": "^1.32.5",
    "axios": "^1.7.2",
    "dotenv": "^16.4.5",
    "ethers": "^6.16.0",
    "express": "^4.21.0",
    "groq-sdk": "^0.37.0",
    "node-fetch": "^2.7.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.23",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.2",
    "@typescript-eslint/eslint-plugin": "^8.18.0",
    "@typescript-eslint/parser": "^8.18.0",
    "eslint": "^9.18.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
